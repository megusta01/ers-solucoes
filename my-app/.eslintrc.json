{
  "extends": "next/core-web-vitals",
  "rules": {
    "eqeqeq": ["error", "always"],
    "no-console": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": ["error", { "max": 1 }],
    "camelcase": "error"
  }
}
